<div class="modal" id="ApproveModal" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="ApproveModalTitle"></h5>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <button id="ApproveModalAnchor" onclick="ApproveModal.Confirm(this)" type="button" class="btn btn-success">
                            <i class="fa fa-check"></i>
                            <span>@Resources.Yes</span>
                        </button>
                        <button type="button" onclick="ApproveModal.Reject(this)" class="btn btn-danger">
                            <i class="fa fa-times"></i>
                            <span>@Resources.No</span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
